{
  "private": true,
  "scripts": {
    "dev": "npm run watch-poll",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "initial-setup": "composer install --prefer-source --no-interaction && php artisan migrate:refresh --force --no-interaction && sleep 30 && php artisan db:seed",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "review-app": "npm run initial-setup && npm run production",
    "eslint": "./node_modules/.bin/eslint resources/js/ --ext .js,.vue"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "laravel-mix": "^4.1.4",
    "resolve-url-loader": "^2.3.2",
    "sass": "^1.26.10",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "bootstrap": "^4.5.2",
    "vue": "^2.6.12",
    "lodash": "^4.17.20",
    "axios": "^0.19.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "css3": "0.0.1",
    "font-awesome": "^4.7.0",
    "stylelint": "^13.6.1",
    "stylelint-scss": "^3.18.0",
    "vue-axios": "^2.1.4",
    "vue-notification": "^1.3.20",
    "vuetify": "^2.3.10",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
